{
  "name": "aframe-depthkit",
  "version": "1.0.0",
  "description": "A port of DepthKit.js volumetric video plugin to AFrame",
  "main": "src/index.js",
  "scripts": {
    "start": "concurrently \"http-server -s --cors\" \"watchify src/index.js -o dist/aframe.depthkit.js -v\"",
    "build": "browserify src/index.js | uglifyjs -mc warnings=false > dist/aframe.depthkit.min.js",
    "test": "npm run build"
  },
  "author": "Or Fleisher <contact@orfleisher.com>",
  "license": "MIT",
  "dependencies": {
    "aframe": "^0.7.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^15.1.0",
    "concurrently": "^3.5.1",
    "uglify-js": "^3.3.5",
    "watchify": "^3.9.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env"
          ]
        }
      ]
    ]
  }
}
